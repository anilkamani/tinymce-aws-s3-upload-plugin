{
  "name": "aws-s3-tinymce-upload",
  "version": "1.0.0",
  "description": "This plugin entegrates a Amazon S3 upload button to your TinyMCE.",
  "main": "dist/aws-s3-tinymce-upload.min.js",
  "scripts": {
    "clean": "rm -rf public",
    "build": "./node_modules/.bin/babel --no-comments app/plugin.js -o dist/tinymce-aws-s3-upload-plugin.min.js",
    "development": "npm run clean && webpack --watch",
    "prepublish": "npm run build"
  },
  "keywords": [
    "amazon-s3-tinymce",
    "aws-s3-upload-tinymce",
    "tinymce",
    "amazon-s3",
    "amazon-s3-upload"
  ],
  "author": "OGPoyraz",
  "license": "MIT",

  "dependencies": {
    "babel-loader": "^6.2.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babili": "0.0.4",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.9.3",
    "path": "^0.12.7",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
